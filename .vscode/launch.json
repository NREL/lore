{
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Django",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}\\loredash\\manage.py",
            "args": [
                "runserver",
                "127.0.0.1:8000",
                "--noreload"
            ],
            "django": true,
            "cwd": "${workspaceFolder}\\loredash"
        },
        {
            "name": "Python: Pytest",
            "type": "python",
            "request": "launch",
            "module": "pytest",
            "cwd": "${workspaceFolder}\\loredash"
        },
        {
            "name": "Python: Bokeh Server",
            "type": "python",
            "request": "launch",
            "module": "bokeh",
            "args": ["serve", "./ui/BokehApps/dashboard_plot",
                // "./ui/BokehApps/historical_dashboard_plot",
                // "./ui/BokehApps/historical_solar_forecast",
                // "./ui/BokehApps/market_plot",
                // "./ui/BokehApps/solar_plot",
                // "./ui/BokehApps/estimates_table.py",
                // "./ui/BokehApps/probability_table.py",
                // "./ui/BokehApps/sliders.py",
                // "./ui/BokehApps/__init__.py",
                "--allow-websocket-origin", "127.0.0.1:8000",
                "--address", "127.0.0.1",
                "--port", "5006",
            ],
            "cwd": "${workspaceFolder}\\loredash"
        }
    ]
}